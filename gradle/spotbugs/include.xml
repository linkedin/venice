      <Bug pattern="DM_NUMBER_CTOR"/>
      <Bug pattern="DM_STRING_CTOR"/>
      <Bug pattern="DM_STRING_TOSTRING"/>
      <Bug pattern="EQ_GETCLASS_AND_CLASS_CONSTANT"/>
